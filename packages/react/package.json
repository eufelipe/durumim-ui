{
  "name": "@durumim-ui/react",
  "version": "0.1.0",
  "description": "A collection of reusable and customizable React components for building user interfaces.",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsup",
    "dev": "tsup src/index.tsx --watch --format esm,cjs --dts --external react",
    "lint": "eslint src/**/*.ts* --fix",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "coverage": "vitest run --coverage"
  },
  "author": "Felipe Rosas (https://eufelipe.com)",
  "license": "MIT",
  "devDependencies": {
    "@durumim-ui/colors": "workspace:*",
    "@durumim-ui/core": "workspace:*",
    "@durumim-ui/eslint": "workspace:*",
    "@durumim-ui/tokens": "workspace:*",
    "@durumim-ui/ts-config": "workspace:*",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/node": "^18.16.3",
    "@types/react": "^18.2.2",
    "@types/react-dom": "^18.2.3",
    "@types/testing-library__jest-dom": "^5.14.5",
    "@vitejs/plugin-react": "^4.0.0",
    "@vitest/coverage-c8": "^0.31.0",
    "@vitest/ui": "^0.31.0",
    "jsdom": "^22.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rollup-plugin-dts": "^5.3.0",
    "tsup": "^6.7.0",
    "typescript": "^5.0.4",
    "vite": "^4.3.4",
    "vitest": "^0.31.0"
  },
  "dependencies": {
    "@stitches/react": "^1.2.8"
  },
  "pnpm": {
    "patchedDependencies": {
      "@types/testing-library__jest-dom@5.14.5": "patches/@types__testing-library__jest-dom@5.14.5.patch"
    }
  },
  "keywords": [
    "UI",
    "React",
    "Components",
    "UI components",
    "Library",
    "Style",
    "Design",
    "Interface",
    "User Interface",
    "Front-end",
    "Web",
    "Development",
    "Javascript",
    "Design system"
  ]
}
